{
  "name": "architecture-v3",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "pnpm css-types:build & vite",
    "build": "pnpm css-types:build && tsc && vite build",
    "preview": "vite preview",
    "check": "biome check --write src",
    "lint": "biome lint --write src",
    "format": "biome format --write src && prettier --write \"src/**/*.scss\"",
    "css-types": "typed-scss-modules \"src/**/*.module.scss\" --watch",
    "css-types:build": "typed-scss-modules \"src/**/*.module.scss\"",
    "prepare": "husky",
    "commit": "cz"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.139.3",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.66.1",
    "react-i18next": "^16.5.4",
    "sonner": "^2.0.7",
    "valibot": "^1.2.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@tanstack/react-router-devtools": "^1.139.3",
    "@types/node": "^24.10.1",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-css-order": "^2.2.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.93.3",
    "typed-scss-modules": "^8.1.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.13"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,jsonc}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "*.{scss,css,sass}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
