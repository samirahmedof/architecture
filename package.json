{
	"name": "architecture-v3",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"sideEffects": false,
	"scripts": {
		"dev": "pnpm css-types & vite",
		"build": "pnpm css-types && tsc && vite build",
		"preview": "vite preview",
		"lint": "biome check src",
		"format": "biome check --apply src",
		"css-types": "typed-scss-modules src --watch --ignore '**/_*.scss'",
		"prepare": "husky"
	},
	"dependencies": {
		"@hookform/resolvers": "^5.2.2",
		"@radix-ui/react-dialog": "^1.1.15",
		"@tanstack/react-query": "^5.90.10",
		"@tanstack/react-router": "^1.139.3",
		"axios": "^1.13.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.19",
		"lucide-react": "^0.554.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-hook-form": "^7.66.1",
		"valibot": "^1.2.0",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@eslint/js": "^9.39.1",
		"@tanstack/react-router-devtools": "^1.139.3",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react-swc": "^4.2.2",
		"eslint": "^9.39.1",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"sass-embedded": "^1.93.3",
		"sass-loader": "^16.0.6",
		"typed-scss-modules": "^8.1.1",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.4",
		"vite": "^7.2.4",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.13"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"biome check --write --no-errors-on-unmatched"
		],
		"*.{json,md}": [
			"biome check --write --no-errors-on-unmatched"
		],
		"*.scss": [
			"prettier --write"
		]
	}
}
